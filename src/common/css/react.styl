//修改element ui 的弹窗默认样式
.el-message-box__wrapper
  background rgba(0,0,45,0.6)
.el-message-box
  width 359px
  height 341px
  border-radius 10px
  background #1a529f
  border 1px solid #1a529f
  .el-message-box__title
    color #fff
    font-size 30px
    margin 30px 0 38px
  .el-message-box__message
    color #fff
    font-size 18px
    margin-bottom 28px
  .el-button--primary
    background rgba(26, 82, 159, 1)
    border 1px solid rgba(255,255,255,0.5)
    width 199px
    height 57px
    font-size 20px
    line-height 30px
    border-radius 29px
  .el-message-box__close,.el-icon-close
    color #FFF
    display none
